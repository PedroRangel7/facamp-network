<?xml version="1.0" encoding="utf-8"?>
<manifest xmlns:android="http://schemas.android.com/apk/res/android"
    package="com.pief.facampnetwork">

    <uses-permission android:name="android.permission.INTERNET" />
    <uses-permission android:name="android.permission.READ_EXTERNAL_STORAGE" />
    <uses-permission android:name="android.permission.WRITE_EXTERNAL_STORAGE" />

    <application
        android:allowBackup="true"
        android:icon="@mipmap/ic_launcher"
        android:label="@string/app_name"
        android:roundIcon="@mipmap/ic_launcher_round"
        android:supportsRtl="true"
        android:theme="@style/Theme.FACAMPNetwork">
        <activity android:name=".RegisterActivity"></activity>
        <activity android:name=".CaronaActivity" />
        <activity android:name=".AulaActivity" />
        <activity android:name=".ProdutoActivity" />
        <activity android:name=".UsuarioActivity" />
        <activity android:name=".AdicionarCaronaActivity" />
        <activity android:name=".AdicionarAulaActivity" />
        <activity android:name=".AdicionarProdutoActivity" />
        <activity
            android:name=".LoginActivity"
            android:label="@string/app_name">
            <intent-filter>
                <action android:name="android.intent.action.MAIN" />

                <category android:name="android.intent.category.LAUNCHER" />
            </intent-filter>
        </activity>
        <activity android:name=".MainActivity" />
    </application>

</manifest>